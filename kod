--[[ funkce main by mela byt posledni definovanou funkci ]]
function main() --Hlavni telo programu
	local a;
	local vyslakshdoiuwdh;
	
	a=main("Zade");
	read("*n");

	if a < 0 then
		a=write("Faw");
	else
		a=factorial(a);
		write("Vy:", vyslakshdoiuwdh, "aaa");
	end;
end;

function funkce()
	write("a");
end;
